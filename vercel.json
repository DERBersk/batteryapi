{
    "version": 2,
    "buildCommand": "python api/app.py",
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/api/app.py"
      }
    ],
    "functions": {
      "api/**/*.py": {
        "memory": 1024,
        "maxDuration": 30,
        "runtime": "@vercel/python"
      }
    },
    "cleanUrls": true,
    "trailingSlash": false
  }
  
