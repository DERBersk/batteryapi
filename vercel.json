{
    "version": 2,
    "buildCommand": "python api/app.py",
    "rewrites":[{
        "source":"/(.*)",
        "destination":"api/app.py"
    }],
    "builds": [
        {
        "src": "api/app.py",
        "use": "@vercel/python"
        }
    ],
    "functions": {
        "api/**/*.py": {
            "maxDuration": 30
    }
    }
}
  