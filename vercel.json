{
  "builds": [
      {
          "src": "app.py",
          "use": "@vercel/python"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "app.py"
      }
  ]
  {
  "functions": {
    "app.py": {
      "maxDuration": 30 // This function can run for a maximum of 30 seconds
    }
  }
}
