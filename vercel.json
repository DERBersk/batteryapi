{
    "version": 2,
    "buildCommand": "vercel build",
    "devCommand": "python3 api/app.py",
    "rewrites":[{
        "source":"/(.*)",
        "destination":"api/app.py"
    }],

    "functions": {
        "api/app.py": {
            "maxDuration": 30
      }
    }
    ,
    "outputDirectory": "."
  }
  