{
  "builds": [
      {
          "src": "app.py",
          "use": "@vercel/python",
          "config": {        "runtime": "python3.8"
        }
      }
  ],
  "functions": {
    "app.py": {
      "maxDuration": 10
    }
  },
  "routes": [
      {
          "src": "/(.*)",
          "dest": "app.py"
      }
  ]
}
