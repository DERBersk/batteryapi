{
    "version": 2,
    "functions":{
        "api/app.py":{
            "maxDuration":60
        }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/app.py"
      }
    ],
    "env": {
      "FLASK_ENV": "production",
      "SECRET_KEY": "your-production-secret-key"
    }
  }
  